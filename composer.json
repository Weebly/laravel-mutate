{
    "name": "weebly/laravel-mutate",
    "description": "Mutate Laravel attributes",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": ["laravel", "eloquent", "uuid", "encrypt", "database"],
    "require": {
        "ramsey/uuid": "~3.0",
        "illuminate/database": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/encryption": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.3",
        "laravel/framework": "5.5.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Weebly\\Mutate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weebly\\Mutate\\": ["tests/Unit/", "tests/Integration/"]
        }
    },
    "authors": [
        {
            "name": "Elliot Fehr",
            "email": "elliot@weebly.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Weebly\\Mutate\\LaravelMutatorServiceProvider"
            ]
        }
    }
}
